    var customJS = serviceChatData.serviceform.customJS;
    if (typeof customJS !== 'undefined') {
      if (customJS !== '') {
        if(sfPixel.checkForJson(customJS)){
          eval(JSON.parse(customJS));
        } else {
          eval(customJS);
        }
      }
    }
  },
