          maxDate: "31/12/2020"
        }).on('blur', function() { // This check is for dd/mm/yyyy format but can be easily adapted to any other
          if(this.value.match(/\d{1,2}[^\d]\d{1,2}[^\d]\d{4,4}/gi) == null){

          }
